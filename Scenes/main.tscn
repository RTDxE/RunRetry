resource_name = "Game"
script/source = "extends Node2D


var enemy = preload(\"res://objects/enemy.tscn\")
var spawn_time = 0.75
var spawn_addict = 0.1
var current_spawn_time = 0
var free_enemies = []

var bottom_right 
var start = false
var score = 0

func _init() -> void:
	TranslationServer.set_locale(\"ru\" if YSDK.get_lang() == \"ru\" else \"en\")

func _ready() -> void:
	randomize()
	update_score()
	set_process(false)
	get_tree().paused = false
	
	YSDK.connect(\"player_logged\", $UI/Start/Login, \"_check_player\")

func _process(delta: float) -> void:
	if current_spawn_time > spawn_time:
		current_spawn_time -= spawn_time
		spawn_enemy()
		spawn_time -= spawn_addict
		spawn_addict *= 0.8
	
	current_spawn_time += delta

func spawn_enemy() -> void:
	var e
	if free_enemies.size() > 0:
		e = free_enemies[0]
		free_enemies.remove(0)
		e.randomize_object()
		e.set_process(true)
		e.passed = false
	else:
		e = enemy.instance()
		e.player = $Player
		add_child(e)
	var top_left = $cam.global_position - get_viewport_rect().size / 2
	bottom_right = $cam.global_position + get_viewport_rect().size / 2
	e.global_position = Vector2(randi() % 824 + 128, top_left.y-300)

func enemy_free(obj) -> void:
	free_enemies.append(obj)


func add_score() -> void:
	score += 1
	update_score()

func update_score() -> void:
	$UI.set_score(score)


func start_game() -> void:
	set_process(true)
	$Player.set_process_input(true)
	$Player.movable = true
	$UI.start_game()
	print(\"StartGame\")
	GameAnalytics.add_progression_event(
		GameAnalytics.PROGRESSION_STATUS.Start, 
		\"infinity\"
	)

func end_game() -> void:
	$Player.die()
	set_process(false)
	get_tree().paused = true
	var record = YSDK.get_record()
	if record >= 0:
		var r = max(score, record)
		if r > record:
			YSDK.set_record(r)
		record = r
	$UI.end_game(score, record)
	print(\"EndGame\")
	GameAnalytics.add_progression_event(
		GameAnalytics.PROGRESSION_STATUS.Complete, 
		\"infinity\", 
		\"\", 
		\"\", 
		score
	)
	yield(get_tree().create_timer(1.5), \"timeout\")
	YSDK.show_fullscreen_adv()

func restart_game() -> void:
	get_tree().reload_current_scene()
"

