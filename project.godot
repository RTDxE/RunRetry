; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Run Retry"
run/main_scene="res://Scenes/main.tscn"
config/icon="res://icon.png"

[autoload]

GameScore="*res://scripts/GameScore.gd"

[display]

window/size/width=1080
window/size/height=1920
window/size/test_width=388
window/size/test_height=691
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="expand"

[locale]

translations=PoolStringArray( "res://tranlation/game.en.translation", "res://tranlation/game.ru.translation" )

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
quality/driver/fallback_to_gles2=true
quality/intended_usage/framebuffer_allocation=0
quality/intended_usage/framebuffer_allocation.mobile=0
2d/snapping/use_gpu_pixel_snap=true
vram_compression/import_etc=true
quality/shadows/filter_mode=0
gles2/compatibility/disable_half_float=true
environment/default_clear_color=Color( 0.952941, 0.952941, 0.952941, 1 )
quality/dynamic_fonts/use_oversampling=false
